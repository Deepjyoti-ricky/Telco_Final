# =====================================================
# Streamlit Secrets Configuration Example
# =====================================================
# Copy this file to secrets.toml and fill in your values
# This file is only needed for local development
# When deployed to Snowflake, connection is automatic

# Snowflake Connection (for local development only)
[snowflake]
account = "your-account-identifier"
user = "your-username"
password = "your-password"
role = "your-role"
warehouse = "your-warehouse"
database = "TELCO_NETWORK_OPTIMIZATION_PROD"
schema = "RAW"

# Optional: Additional configurations
[app]
debug_mode = false
cache_ttl = 300  # Cache time-to-live in seconds

